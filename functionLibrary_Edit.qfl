Function checkforcharsinString(strValue)

	Set re = New RegExp
	re.Pattern = "[a-z]"
	re.IgnoreCase = True
	re.Global = True
	hasMatches = re.Test(strValue)
	
	If hasMatches = True Then
		numberArr = getNumbersFromString(strValue)
	End If
	
End Function

Function verifyIsEmpty(strValue)
    
    If strValue = "Spool NO" Then
        If IsEmpty(spoolNo) Then
            actualResult = "Print packing successful. Spool No = " & spoolNo
            verifyIsEmpty = 0
        Else
            actualResult = "Print Packing failed. Spool Number is not generated."
            verifyIsEmpty = 1
        End If
	
    ElseIf strValue = "Baseline Date" Then
        If IsEmpty(baselineDate) Then
            actualResult = "Baseline Date field empty. Invoicing not successful."
            verifyIsEmpty = 0
        Else
            actualResult = "Baseline Date = " & baselineDate
            verifyIsEmpty = 1
        End If
    End If
    
    ElseIf strValue = "Payment Terms" Then
        If IsEmpty(pmntTerms) Then
            actualResult = "Payment Terms field empty. Invoicing not successful."
            verifyIsEmpty = 0
        Else
            actualResult = "Payment Terms = " & pmntTerms
            verifyIsEmpty = 1
        End If
    End If
    
End Function

